{
  "name": "jike-cli",
  "version": "0.0.1",
  "description": "Jike CLI",
  "homepage": "https://github.com/open-jike/jike-cli#readme",
  "bugs": {
    "url": "https://github.com/open-jike/jike-cli#issues"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": "./bin/jike-cli.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-jike/jike-cli.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "esno src/index.ts",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --fix --ext .vue,.js,.ts,.jsx,.tsx",
    "build": "tsup",
    "test": "vitest",
    "release": "bumpp --commit --push --tag && npm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@poppinss/cliui": "^3.0.1",
    "@vue-reactivity/fs": "^0.1.1",
    "commander": "^9.1.0",
    "enquirer": "^2.3.6",
    "jike-sdk": "^0.7.3"
  },
  "devDependencies": {
    "@sxzz/eslint-config-prettier": "^2.1.0",
    "@sxzz/eslint-config-ts": "^2.1.0",
    "@types/node": "*",
    "bumpp": "^7.1.1",
    "eslint": "^8.11.0",
    "eslint-define-config": "^1.3.0",
    "esno": "^0.14.1",
    "fast-glob": "^3.2.11",
    "prettier": "^2.6.0",
    "tsup": "^5.12.1",
    "typescript": "^4.6.2",
    "vitest": "^0.7.10"
  }
}
